{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },

  "SeedingOptions": {
    "UseExternalSource": true,
    "DataFolderPath": "Data/SeedDatabase/JsonData",
    "ProductJsonFile": "products.json",
    "StoreJsonFile": "stores.json",
    "PriceJsonFile": "prices.json",
    "StrictValidation": true,
    "EnableValidationLogging": true,
    "MaxValidationErrorsToLog": 50,
    "EnabledSeeders": {
      "Product": true,
      "Store": true,
      "Price": true,
      "Role": true,
      "User": true,
      "UserRole": true,
      "Expense": true,
      "Budget": true,
      "Task": true,
      "Notification": true
    }
  }
}